<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="100.0" prefWidth="500.0" stylesheets="/styles/lobby/lobby-item.css" styleClass="item"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:id="lobbyItem">
    <HBox prefHeight="100.0" prefWidth="500.0" styleClass="hSpacer" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <VBox prefHeight="50.0" prefWidth="400.0" styleClass="vSpacer">
            <Label fx:id="lbName" styleClass="lobbyTitle"/>
        </VBox>
    </HBox>
</AnchorPane>
