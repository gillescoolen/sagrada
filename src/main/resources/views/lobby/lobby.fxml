<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<HBox prefHeight="720" prefWidth="1200" stylesheets="/styles/lobby/lobby-list.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <VBox prefHeight="720.0" prefWidth="350.0" styleClass="side-column">
        <Text styleClass="title" text="Invites" />
        <VBox fx:id="vbLobbyInvites" styleClass="lobby-list" />
    </VBox>
    <VBox prefHeight="720.0" prefWidth="850.0" styleClass="main-column">
        <Button fx:id="btnCreateGame" text="Create game" />
        <Text styleClass="title" text="Lobby's" />
        <VBox fx:id="vbLobbyGames" styleClass="lobby-list" />
    </VBox>
</HBox>
