<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Text?>

<AnchorPane styleClass="priv-obj-card-wrapper" stylesheets="/styles/game/private-objective-card.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml">
    <VBox styleClass="priv-obj-card-vbox" prefHeight="125.0" prefWidth="150.0" maxWidth="150">
        <Text fx:id="name" styleClass="priv-obj-card-title" wrappingWidth="150.0"/>
        <Text fx:id="description" wrappingWidth="150.0"/>
    </VBox>
    <Rectangle fx:id="color" styleClass="private-dice" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" height="40" width="40"/>
</AnchorPane>
